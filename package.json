{
  "name": "ddb",
  "version": "1.1.0",
  "description": "Effortless aerial data management and sharing",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha nodejs/test/*.js",
    "install": "node scripts/platform-detect.js && cmake-js compile --prefer-make --CDVCPKG_HOST_TRIPLET=${VCPKG_TRIPLET:-x64-windows-release} --CDVCPKG_TARGET_TRIPLET=${VCPKG_TRIPLET:-x64-windows-release} --CDCMAKE_BUILD_TYPE=Release --CDBUILD_TESTING=ON --CDCMAKE_TOOLCHAIN_FILE=./vcpkg/scripts/buildsystems/vcpkg.cmake",
    "parallel_install": "node scripts/platform-detect.js && CMAKE_BUILD_PARALLEL_LEVEL=4 cmake-js compile --prefer-make --CDVCPKG_HOST_TRIPLET=${VCPKG_TRIPLET:-x64-windows-release} --CDVCPKG_TARGET_TRIPLET=${VCPKG_TRIPLET:-x64-windows-release} --CDCMAKE_BUILD_TYPE=Release --CDBUILD_TESTING=ON --CDCMAKE_TOOLCHAIN_FILE=./vcpkg/scripts/buildsystems/vcpkg.cmake"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DroneDB/DroneDB.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DroneDB/DroneDB/issues"
  },
  "homepage": "https://github.com/DroneDB/DroneDB#readme",
  "devDependencies": {
    "cmake-js": "^6.3.0",
    "mocha": "^7.2.0",
    "nan": "^2.16.0"
  }
}